@using Egharpay.Business.Extensions
@using Egharpay.Extensions
@{
    var layout = User.IsSuperUserOrAdmin() ? "~/Views/Shared/_Layout.cshtml" : "~/Views/Shared/_Layout_2.cshtml";
    Layout = layout;
    ViewBag.Title = "Joinus";
}
@*<h2>Join us</h2>*@
<div class="row" ng-controller="MobileRepairController as model">
    <div class="col-lg-6">
        <div class="ibox float-e-margins">
            <div class="ibox-content">
              <img class="join-us" src="~/Images/join-us.jpg"/>
            </div>
        </div>
    </div>
    <div class="col-lg-6">
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5>Join us and become mumbile partner.<small>mumbile partner programme.</small></h5>
            </div>
            @Html.Partial("_Partner")
        </div>
    </div>
</div>


